#include <stdio.h>

typedef struct _node{
	struct _node* _next;
	struct _node* _prev;
	void*  _data;
}node;

void addNode(struct node** list, void** data)
{

	if ( (*list) == NULL )
	{
		(*list) = (node*) malloc(sizeof(struct node));
		(*list)->_data = data;
		strcpy( (*list)->name, name );

		(*list)->_next = NULL;
		(*list)->_prev = NULL;
	}
	else
		addNode( (&(*list)->next), name, sock_id);
}


